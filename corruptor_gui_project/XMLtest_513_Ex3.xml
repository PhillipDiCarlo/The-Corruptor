<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<test>
  <event>
    <variant>EXIT_ON_REACQ</variant>
    <constraint>
      <type>time_until_intercept</type>
      <bounds>CONSTRAINT_BOUND_LESSER</bounds>
      <value>6.0</value>
    </constraint>
  </event>
  <event>
    <variant>LINK_ERRORS</variant>
    <constraint>
      <type>time_until_intercept</type>
      <bounds>CONSTRAINT_BOUND_LESSER</bounds>
      <value>5.0</value>
    </constraint>
  </event>
  <event>
    <variant>ACQ_LINK_ERRORS</variant>
    <constraint>
      <type>rate_change</type>
      <bounds>CONSTRAINT_BOUND_LESSER_OR_EQUAL</bounds>
      <value>4.0</value>
    </constraint>
  </event>
  <event>
    <variant>UPLINK_LATE</variant>
    <constraint>
      <type>dwell_cnt</type>
      <bounds>CONSTRAINT_BOUND_LESSER</bounds>
      <value>6.0</value>
    </constraint>
    <constraint>
      <type>link_sequence_done</type>
      <bounds>CONSTRAINT_BOUND_LESSER_OR_EQUAL</bounds>
      <value>7.0</value>
    </constraint>
    <constraint>
      <type>uplink_lost_inject_count</type>
      <bounds>CONSTRAINT_BOUND_EQUAL</bounds>
      <value>8.0</value>
    </constraint>
  </event>
  <event>
    <variant>SEND_SDM_PARAMETERS</variant>
    <constraint>
      <type>acq_complete_flag</type>
      <bounds>CONSTRAINT_BOUND_GREATER</bounds>
      <value>4.0</value>
    </constraint>
    <constraint>
      <type>time_of_flight</type>
      <bounds>CONSTRAINT_BOUND_LESSER_OR_EQUAL</bounds>
      <value>7.0</value>
    </constraint>
  </event>
  <event>
    <variant>PHASE_TRANSITION_UPLINK_ERRORS</variant>
    <constraint>
      <type>acquisition_count</type>
      <bounds>CONSTRAINT_BOUND_LESSER_OR_EQUAL</bounds>
      <value>6.0</value>
    </constraint>
  </event>
</test>
