<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<test>
  <event>
    <variant>ACQ_LINK_ERRORS</variant>
    <constraint>
      <type>acq_complete_flag</type>
      <bounds>CONSTRAINT_BOUND_GREATER_OR_EQUAL</bounds>
      <value>6.0</value>
    </constraint>
    <link_manifest>
      <step>
        <link_error_count>5.0</link_error_count>
        <cooldown_time>6.0</cooldown_time>
        <corruption_type>UL_XMIT_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>8.0</link_error_count>
        <cooldown_time>55.0</cooldown_time>
        <corruption_type>UL_T3_ERROR</corruption_type>
        <corruption_type>UL_T4_ERROR</corruption_type>
        <corruption_type>UL_ERROR</corruption_type>
      </step>
    </link_manifest>
  </event>
  <event>
    <variant>ACQ_SUBPULSE_ERRORS</variant>
    <constraint>
      <type>dwell_cnt</type>
      <bounds>CONSTRAINT_BOUND_LESSER</bounds>
      <value>6.0</value>
    </constraint>
    <link_manifest>
      <step>
        <link_error_count>7.0</link_error_count>
        <cooldown_time>1.0</cooldown_time>
        <corruption_type>UL_T2_ERROR</corruption_type>
      </step>
    </link_manifest>
  </event>
  <event>
    <variant>EXIT_AFTER_ZONE</variant>
    <constraint>
      <type>endless_event_flag</type>
      <bounds>CONSTRAINT_BOUND_EQUAL</bounds>
      <value>20.0</value>
    </constraint>
    <link_manifest>
      <step>
        <link_error_count>30.0</link_error_count>
        <cooldown_time>21.0</cooldown_time>
        <corruption_type>UL_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>15.0</link_error_count>
        <cooldown_time>10.0</cooldown_time>
        <corruption_type>SET_SEQUENTIAL</corruption_type>
        <corruption_type>SET_INHIBITED</corruption_type>
      </step>
      <step>
        <link_error_count>3.0</link_error_count>
        <cooldown_time>9.0</cooldown_time>
        <corruption_type>UL_XMIT_ERROR</corruption_type>
      </step>
    </link_manifest>
  </event>
  <event>
    <variant>EXIT_ON_REACQ</variant>
    <constraint>
      <type>time_of_flight</type>
      <bounds>CONSTRAINT_BOUND_GREATER</bounds>
      <value>50.0</value>
    </constraint>
    <link_manifest>
      <step>
        <link_error_count>6.0</link_error_count>
        <cooldown_time>20.0</cooldown_time>
        <corruption_type>UL_T4_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>5.0</link_error_count>
        <cooldown_time>13.0</cooldown_time>
        <corruption_type>SET_FORCED</corruption_type>
        <corruption_type>UL_T4_ERROR</corruption_type>
      </step>
    </link_manifest>
  </event>
  <event>
    <variant>EXIT_ON_TRANSITION</variant>
    <constraint>
      <type>link_event_done</type>
      <bounds>CONSTRAINT_BOUND_GREATER_OR_EQUAL</bounds>
      <value>4.0</value>
    </constraint>
    <link_manifest>
      <step>
        <link_error_count>8.0</link_error_count>
        <cooldown_time>10.0</cooldown_time>
        <corruption_type>UL_XMIT_ERROR</corruption_type>
        <corruption_type>UL_T2_ERROR</corruption_type>
        <corruption_type>UL_T3_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>10.0</link_error_count>
        <cooldown_time>40.0</cooldown_time>
        <corruption_type>DL_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>1.0</link_error_count>
        <cooldown_time>83.0</cooldown_time>
        <corruption_type>UL_GID_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>70.0</link_error_count>
        <cooldown_time>14.0</cooldown_time>
        <corruption_type>CROSS_CORR</corruption_type>
        <corruption_type>UL_XMIT_ERROR</corruption_type>
      </step>
      <step>
        <link_error_count>10.0</link_error_count>
        <cooldown_time>13.0</cooldown_time>
        <corruption_type>UL_T3_ERROR</corruption_type>
      </step>
    </link_manifest>
  </event>
  <event>
    <variant>PHASE_THREE_LINK_ERRORS</variant>
    <constraint>
      <type>rate_change</type>
      <bounds>CONSTRAINT_BOUND_GREATER_OR_EQUAL</bounds>
      <value>30.0</value>
    </constraint>
    <link_manifest>
      <step>
        <link_error_count>39.0</link_error_count>
        <cooldown_time>15.0</cooldown_time>
        <corruption_type>UL_XMIT_ERROR</corruption_type>
      </step>
    </link_manifest>
  </event>
</test>
